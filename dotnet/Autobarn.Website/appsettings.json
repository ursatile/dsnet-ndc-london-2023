{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "AutobarnSqlConnectionString": "Server=localhost;Database=Autobarn;User=autobarn;Password=autobarn;MultipleActiveResultSets=true;",
    "AutobarnRabbitMQ": "amqp://user:pass@localhost:5672"
  },
  "DatabaseMode": "csv",
  // set this to "csv" to use the in-memory data store backed by CSV files
  "AllowedHosts": "*"
}
